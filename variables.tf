variable "channel" {
  description = "The chat channel to send the message in"
  type        = string
}

variable "message" {
  description = "The chat message to send"
  type        = string
}

variable "paperkey" {
  description = "The paperkey of your bot's Keybase account"
  type        = string
}

variable "username" {
  description = "The username of your bot's Keybase account"
  type        = string
}
